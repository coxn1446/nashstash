{
  "name": "nashstash",
  "version": "1.0.0",
  "description": "Nash Stash - A collection of games for the App Store",
  "main": "serverIndex.js",
  "scripts": {
    "start": "node serverIndex.js",
    "start:dev": "HTTPS=true SSL_CRT_FILE=secrets/cert.pem SSL_KEY_FILE=secrets/key.pem react-scripts start",
    "server:dev": "nodemon serverIndex.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "s": "npx cap sync ios",
    "check-secrets": "node scripts/check-secrets.js",
    "deploy:qa": "gcloud app deploy app-qa.yaml"
