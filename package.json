    "selfsigned": "^2.4.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^4.9.5",
    "web-vitals": "^3.5.2",
    "yaml": "^2.8.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
