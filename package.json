    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.0",
    "express-rate-limit": "^7.0.0",
    "express-session": "^1.17.0",
    "firebase": "^12.6.0",
    "firebase-admin": "^12.0.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.0",
